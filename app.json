{
  "expo": {
    "name": "Richmond School District Parent App",
    "slug": "richmond-school-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon.png",
    "splash": {
      "image": "./assets/richsplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7A5580"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon-adaptive.png",
        "backgroundColor": "#7A5580"
      }
    }
  }
}
